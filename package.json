{
    "name": "p7zip-threetwo",
    "version": "1.0.3",
    "description": "A node wrapper for 7z including latest version of 7za",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "os": [
        "darwin",
        "linux"
    ],
    "engines": {
        "node": ">= 12.0.0"
    },
    "scripts": {
        "test": "jest --silent=false --config=jest.json --testRegex='/src/.*\\.test\\.ts$'",
        "install": "node scripts/install.js",
        "lint": "tslint -c ./tslint.json -p ./tsconfig.json -t verbose",
        "build": "tsc -p ./tsconfig.json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jbdemonte/node-p7zip.git"
    },
    "author": {
        "name": "Rishi Ghan",
        "email": "rishi.ghan@gmail.com",
        "url": "https://rishighan.com"
    },
    "keywords": [
        "typescript",
        "7z",
        "7za",
        "p7zip",
        "7zip",
        "wrapper",
        "bin"
    ],
    "license": "MIT",
    "dependencies": {
        "decompress": "^4.2.1"
    },
    "devDependencies": {
        "@gojob/tslint-config": "^1.0.0",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.10.1",
        "coveralls": "^3.1.1",
        "jest": "^27.2.3",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.3.0",
        "typescript": "^4.4.3"
    },
    "pre-commit": [
        "lint"
    ]
}
